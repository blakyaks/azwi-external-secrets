apiVersion: external-secrets.io/v1beta1
kind: ExternalSecret
metadata:
  name: external-secret
  namespace: default
spec:
  refreshInterval: 1m
  secretStoreRef:
    kind: ClusterSecretStore
    name: azure-cluster-store
  target:
    creationPolicy: Owner
    deletionPolicy: Delete
  data:
    - secretKey: external-secret
      remoteRef:
        key: external-keyvault-secret